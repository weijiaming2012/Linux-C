#include<stdio.h>

int fac_for(int n)
{
	int fac=1,i;
	for(i=1;i<=n;i++)
	{
		fac*=i;
	}
	return fac;
}

int fac(int n)
{
	float f;
	if(n<0)
	{
		printf("n<0,data error!");
		return -1;
	}
	else if(n==0|| n==1)
		f=1;
	else
		f=n*fac(n-1);
	return f;
}

int main()
{
	int n,f1,f2;
	scanf("%d",n);
	f1=fac_for(n);
	f2=fac(n);
	printf("%d!=%d\n",f1);
	return 0;
}
